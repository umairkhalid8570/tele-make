		<xpath expr="//we-select[@data-variable='btn-secondary-outline']" position="after">
			<div data-selector=".biz_dynamic_product_slider, .oe_multi_category_slider, .bizcommon_category_slider_3, .blog_slider_owl, .brand_slider">
	            <we-select string="Sliders Width" data-variable="slider-width">
            		<we-button data-select-class="default_style">Default Size</we-button>
                 	<we-button data-select-class="extended-container">Extended size</we-button>
	            </we-select>
		    </div>
		    <div data-js="config_slider_clone_fix" data-selector="#wrapwrap" data-no-check="true"/>
		</xpath>

		<xpath expr="//div[@data-selector='#wrapwrap > header nav.navbar .navbar-brand img']" position="attributes">
			<attribute name="data-selector">#wrapwrap > header .navbar-brand img</attribute>
		</xpath>
	</template>

	<template id="icon_selector_inherit" inherit_id="web_editor.snippet_options">
		<xpath expr="//div[@data-js='ReplaceMedia']" position="attributes">
			<attribute name="data-selector">img, .media_iframe_video, span.fa, i.fa, .lnr, .icon, .icofont, .lni, .ri, .ti</attribute>
		</xpath>

		<xpath expr="//div[@data-js='FontawesomeTools']" position="attributes">
			<attribute name="data-selector">span.fa, i.fa, .lnr, .icon, .icofont, .lni, .ri, .ti</attribute>
		</xpath>
		
		<xpath expr="//div[@data-selector='span.fa, i.fa, img']" position="attributes">
			<attribute name="data-selector">span.fa, i.fa, img, .lnr, .icon, .icofont, .lni, .ri, .ti</attribute>
		</xpath>
	</template>

