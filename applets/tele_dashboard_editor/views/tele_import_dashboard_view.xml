<tele>
    <data>

        <record id="tele_import_dashboard_form_view" model="ir.ui.view">
            <field name="name">tele_dashboard_editor.import form</field>
            <field name="model">tele_dashboard_editor.import</field>
            <field name="arch" type="xml">
                 <form string="Import Dashboard">
                <group>
                    <group>
                        <field name="tele_import_dashboard" string="Upload Dashboard Json" required="1"/>
                        <field name="tele_top_menu_id" string="Show Under Menu" required="1"/>
                    </group>
                </group>
                <footer>
                    <button name="tele_do_action" string="Save" type="object" class="btn-primary"/>
                    <button string="Cancel" class="btn-secondary" special="cancel"/>
                </footer>
            </form>
            </field>
        </record>


        <record id="tele_import_dashboard_action" model="ir.actions.act_window">
            <field name="name">Import Dashboard</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">tele_dashboard_editor.import</field>
            <field name="view_mode">form</field>
             <field name="target">new</field>
        </record>

        <menuitem name="Import Dashboards" id="tele_dashboard_editor.tele_import_dashboard_menu"
                  parent="tele_dashboard_editor.configuration_menu"
                  action="tele_dashboard_editor.tele_import_dashboard_action" groups="base.group_system"/>

<!--                <menuitem name="Dashboard Import" id="tele_dashboard_editor.dashboard_layout_menu"-->
<!--                  parent="tele_dashboard_editor.configuration_menu"-->
<!--                  action="tele_dashboard_editor.layout_tree_action_window" groups="tele_dashboard_editor_group_manager"/>-->
    </data>

</tele>